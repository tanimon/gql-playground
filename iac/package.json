{
  "name": "iac",
  "version": "0.0.0",
  "bin": {
    "iac": "bin/iac.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "check": "run-p check:*",
    "check:format": "prettier --cache -c '**/*.{ts,json,yml,yaml,md}'",
    "check:lint": "eslint --cache --max-warnings 0 '**/*.ts'",
    "check:tsc": "tsc",
    "fix": "run-s fix:format fix:lint check:tsc",
    "fix:format": "prettier --cache --write '**/*.{ts,json,yml,yaml,md}'",
    "fix:lint": "eslint --cache --max-warnings 0 --fix '**/*.{ts,tsx}'",
    "sort": "sort-package-json",
    "test": "jest",
    "watch": "tsc -w"
  },
  "dependencies": {
    "aws-cdk-lib": "2.73.0",
    "constructs": "10.0.0",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@types/node": "18.14.6",
    "aws-cdk": "2.73.0",
    "jest": "29.5.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": ".9.5"
  }
}
