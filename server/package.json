{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "check": "run-p check:*",
    "check:format": "prettier --cache -c '**/*.{ts,json,yml,yaml,md}'",
    "check:lint": "eslint --cache --max-warnings 0 '**/*.ts'",
    "check:tsc": "tsc",
    "check:tsc:watch": "tsc --watch",
    "cli": "npm run start ./src/cli/main.ts",
    "fix": "run-s fix:format fix:lint check:tsc",
    "fix:format": "prettier --cache --write '**/*.{ts,json,yml,yaml,md}'",
    "fix:lint": "eslint --cache --max-warnings 0 --fix '**/*.{ts,tsx}'",
    "sort": "sort-package-json",
    "start": "node -r esbuild-register",
    "test": "jest",
    "test:watch": "jest --watch",
    "update:pkgs": "ncu -u"
  },
  "dependencies": {
    "@aws-sdk/lib-dynamodb": "3.282.0",
    "dayjs": "1.11.7",
    "fs-extra": "11.1.0",
    "inversify": "6.0.1",
    "lodash.chunk": "4.2.0",
    "lodash.orderby": "4.6.0",
    "lodash.sortby": "4.7.0",
    "source-map-support": "0.5.21",
    "undici": "5.20.0",
    "uuid": "9.0.0",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@classmethod/aws-util": "file:../shared/aws-util",
    "@types/fs-extra": "11.0.1",
    "@types/inquirer": "9.0.3",
    "@types/jest": "29.4.0",
    "@types/lodash.chunk": "4.2.7",
    "@types/lodash.orderby": "4.6.7",
    "@types/lodash.sortby": "4.7.7",
    "@types/source-map-support": "0.5.6",
    "@types/uuid": "9.0.1",
    "dotenv": "16.0.3",
    "inquirer": "9.1.4",
    "jest": "29.4.3"
  }
}
